{
    "base_path" : "/"
    ,
    "date_format" : "%Y-%m-%d"
    ,
    "compiled_path" : "compiled"
    ,
    "_root" : {
        "permalink" : "/:relative_path/:filename",
        "paginator" : {
            "url" : "/index/"
        },
        "rss" : {
            "url" : "/"
        }
    }
    ,
    "compiled" : {
        "use" : "ignore"
    }
    ,
    "plugins" : {
        "use" : "ignore"
    }
    ,
    "media" : {
        "use" : "static"
    }
    ,    
    "data" : {
        "use" : "data"
    }
    ,
    "javascripts" : {
        "use" : "javascripts"
    }
    ,
    "layouts" : {
        "use" : "layouts"
    }
    ,
    "partials" : {
        "use" : "partials"
    }
    ,
    "stylesheets" : {
        "use" : "stylesheets"
    }
    ,
    "widgets" : {
        "use" : "widgets"
    }
}
